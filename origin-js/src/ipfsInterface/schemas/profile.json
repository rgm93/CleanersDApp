{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$id": "http://schema.originprotocol.com/profile_v1.0.0",
  "title": "Origin Protocol user profile schema",
  "type": "object",
  "properties": {
    "schemaId": {
      "type": "string"
    },
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "roleName": {
      "type": "string"
    },
    "username": {
      "type": "string"
    },
    "password": {
      "type": "string"
    },
    "postalCode": {
      "type": "string"
    },
    "nif": {
      "type": "string"
    },
    "avatar": {
      "type": "string"
    }
  },
  "required": [
    "schemaId",
    "firstName",
    "lastName",
    "roleName",
    "username",
    "password",
    "postalCode",
    "nif"
  ]
}
